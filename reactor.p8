pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function draw_trail()
	-- trail tile
	local t=22
	-- col sequence
	local cols={5,6,7,10,9,8}
	-- anim speed
	local speed=0.5

 foreach(trail,function(o)
		if o.t>=#cols then
		 mset(o.x,o.y,t)
			o=nil
		else
		 rectfill(o.x*8,o.y*8,-1+(1+o.x)*8,-1+(1+o.y)*8,cols[flr(o.t)])
			o.t+=speed
		end
 end)
end

function create_player(x,y,player)
	local o={}
	-- player
	o.player=player
	-- frame inside anim
	o.frame=1
	-- currently moving
	o.move=false
	-- animation sets
	o.anim={{7,8,9,8},{10,11,12,11},{4,5,4,6},{1,2,1,3}}
	-- anim speed
	o.speed=0.5
	-- speed of movement
	o.pace=1
	-- direction
	o.face=4
	-- actual coords
	o.x=x
	o.y=y
	-- map coords
	o.mx=x/8
	o.my=y/8
	return o
end

function _init()
	man=create_player(64,64,1)
	trail={}
end


function _update()
 move_man(man)
end

function _draw()
	--draw sky
	cls(5)
	rectfill(0,0,128,16,9)
	rectfill(0,17,128,24,10)
	rectfill(0,25,128,25,10)
	rectfill(0,18,128,18,9)
	rectfill(0,0,128,9,8)
	rectfill(0,11,128,12,8)
	rectfill(0,15,128,15,8)
	palt(7,true)
	palt(0,false)
	map(0,2,0,16,16,2)
	palt()
	map(0,4,0,32,16,12)
	draw_trail()
	draw_bg()
 draw_man(man) 
end

function move_man(o)
	local a=false
	if(not o.move) then
		if((btn(0) and not btn(1)) or (not btn(0) and btn(1))) then
		 o.move=true
			if(btn(0)) then 
				o.face=1
			else 
				o.face=2
			end
		end
		if((btn(2) and not btn(3)) or (not btn(2) and btn(3))) then
		 o.move=true
			if(btn(2)) then 
				o.face=3
			else 
				o.face=4
			end
		end
	end
	if o.move then
	 -- movement
		if(o.face==1) then 
		 o.x-=o.pace
		elseif(o.face==2) then
		 o.x+=o.pace
		elseif(o.face==3) then
		 o.y-=o.pace
		else
		 o.y+=o.pace
		end
		-- end movement on grid
		if(o.x%8==0 and o.y%8==0) then 
			o.move=false
			add(trail,{x=o.mx,y=o.my,t=1})
			-- save new map position
			o.mx=o.x/8
			o.my=o.y/8
		end
		-- animation
		o.frame += o.speed
		o.frame -= 1
		o.frame %= #o.anim[o.face]
		o.frame += 1
	end
end

function draw_man(o)
	spr(o.anim[o.face][flr(o.frame)],o.x,o.y,1,1)
end

function draw_bg()
	spr(32,14*8,14*8)
	spr(33,15*8,14*8)
	spr(48,14*8,15*8)
	spr(49,15*8,15*8)
end
